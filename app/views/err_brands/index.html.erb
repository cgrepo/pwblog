<p id="notice"><%= notice %></p>

<h1>Listing Err Brands</h1>

<table>
  <thead>
    <tr>
      <th>Brand</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @err_brands.each do |err_brand| %>
      <tr>
        <td><%= err_brand.brand %></td>
        <td><%= err_brand.description %></td>
        <td><%= link_to 'Show', err_brand %></td>
        <td><%= link_to 'Edit', edit_err_brand_path(err_brand) %></td>
        <td><%= link_to 'Destroy', err_brand, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Err brand', new_err_brand_path %>
